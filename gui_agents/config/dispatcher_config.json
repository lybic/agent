{
  "dispatcher": {
    "enable_quality_monitoring": true,
    "enable_cost_tracking": true,
    "enable_adaptive_execution": true,
    "quality_check_interval": 60.0,
    "cost_alert_threshold": 0.8,
    "max_consecutive_failures": 3,
    "enable_visual_monitoring": false,
    "log_all_interactions": true,
    "debug_mode": false
  },
  "quality_checks": {
    "check_interval": 60.0,
    "step_interval": 10,
    "include_progress_analysis": true,
    "include_efficiency_check": true,
    "screenshot_analysis": false,
    "deep_reasoning": false,
    "use_lightweight_model": true,
    "estimated_cost": 0.05,
    "triggers": {
      "enable_time_trigger": true,
      "enable_step_trigger": true,
      "enable_error_trigger": true,
      "enable_failure_trigger": true,
      "error_threshold": 2,
      "failure_threshold": 1
    }
  },
  "cost_budget": {
    "total_limit": 10.0,
    "per_hour_limit": 5.0,
    "per_task_limit": 2.0,
    "quality_check_budget": 1.0,
    "warning_threshold": 0.8,
    "stop_threshold": 0.95,
    "enable_cost_alerts": true,
    "enable_auto_stop": false
  },
  "adaptive_execution": {
    "enable_conservative_mode": true,
    "enable_granular_subtasks": true,
    "enable_auto_replan": true,
    "max_adaptations_per_task": 5,
    "adaptation_cooldown": 30.0,
    "effectiveness_threshold": 0.6
  },
  "monitoring": {
    "screenshot_cache_size": 10,
    "interaction_log_limit": 100,
    "quality_report_limit": 50,
    "enable_performance_tracking": true,
    "enable_failure_analysis": true,
    "cleanup_interval_days": 7
  },
  "tools": {
    "reflector_tool": "traj_reflector",
    "lightweight_vision_tool": "lightweight_vision",
    "fallback_on_tool_failure": true,
    "tool_timeout": 30.0
  }
} 